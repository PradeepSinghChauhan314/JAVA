package calculator;
import java.util.*;
public class Factorial {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter a number");
		int n=sc.nextInt();
		int a;
		for (i=1;i<=n;i++)
		{
		   a=a+i;
		}
		System.out.println("the factorial is:"+a);
		
		
		
	}

}
