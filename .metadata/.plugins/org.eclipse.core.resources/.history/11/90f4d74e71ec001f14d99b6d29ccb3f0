package calculator;
import java.util.*;
public class Grade {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the Grade");
		
		String X;
		switch("X")
		{
		case "A":
			System.out.println("Student has scored between 90 to 100");
			break;
		case "B":
			System.out.println("Studend has scored between 75 to 90");
			break;
		case "C":
			System.out.println("Student has scored between 60 to 75");
			break;
		case "D":
			System.out.println("Student has scored between 45 to 60");
			break;
	    default:
				System.out.println("The entered grade is not valid");
				
		}
	}

}
